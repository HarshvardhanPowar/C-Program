#include<stdio.h>

int main()
{
    int a=4; //Integral Variables
    int d=45; //Intergral Vriables
    float b=8.5; // Real Variable
    cher c='u'; // Character Vriables
    int e=45+4; //Intergral Variables
    
    printf("The value of a is %d",&a);
    printf("The Value of b is %f",&b);
    printf("The value of c is %c",&c);
    printf("The Value Of d is %d",&d);
    printf("the value of e is %d",&e);

    return 0;
    
}
